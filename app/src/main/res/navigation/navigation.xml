<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="kz.smartideagroup.partner.content.view.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_cashBoxFragment"
            app:destination="@id/cashBoxFragment" />
    </fragment>
    <fragment
        android:id="@+id/deliveryFragment"
        android:name="kz.smartideagroup.partner.content.view.delivery.DeliveryFragment"
        android:label="DeliveryFragment"
        tools:layout="@layout/fragment_delivery">
        <action
            android:id="@+id/action_deliveryFragment_to_menuFragment"
            app:destination="@id/menuFragment" />
        <action
            android:id="@+id/action_deliveryFragment_to_detailPageFragment"
            app:destination="@id/detailPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationFragment"
        android:name="kz.smartideagroup.partner.content.view.notification.NotificationFragment"
        android:label="NotificationFragment"
        tools:layout="@layout/fragment_notification"/>
    <fragment
        android:id="@+id/reportsFragment"
        android:name="kz.smartideagroup.partner.content.view.reports.ReportsFragment"
        android:label="ReportsFragment"
        tools:layout="@layout/fragment_reports"/>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="kz.smartideagroup.partner.content.view.settings.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_aboutUsFragment"
            app:destination="@id/aboutUsFragment" />
    </fragment>
    <fragment
        android:id="@+id/cashBoxFragment"
        android:name="kz.smartideagroup.partner.content.view.home.CashBoxFragment"
        android:label="CashBoxFragment"
        tools:layout="@layout/fragment_cash_box"/>
    <fragment
        android:id="@+id/menuFragment"
        android:name="kz.smartideagroup.partner.content.view.delivery.MenuFragment"
        android:label="MenuFragment"
        tools:layout="@layout/fragment_menu">
        <action
            android:id="@+id/action_menuFragment_to_deliveryFragment"
            app:destination="@id/deliveryFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutUsFragment"
        android:name="kz.smartideagroup.partner.content.view.settings.AboutUsFragment"
        android:label="AboutUsFragment" />
    <fragment
        android:id="@+id/detailPageFragment"
        android:name="kz.smartideagroup.partner.content.view.delivery.DetailPageFragment"
        android:label="DetailPageFragment" />
</navigation>